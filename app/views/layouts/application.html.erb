<!DOCTYPE html>
<html>
  <head>
    <title>Film Twitic</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>

    <div class="container">
      <% flash.each do |name, msg| %>
        <div class="<%= name %>">
          <%= msg %>
        </div>

      <% end %>

      <%= yield %>
    </div>

  </body>
</html>
